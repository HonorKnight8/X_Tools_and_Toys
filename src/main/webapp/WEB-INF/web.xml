<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!-- file_name -->
    <servlet>
        <servlet-name>file_name</servlet-name>
        <servlet-class>monster.helloworld.file_name.FileName</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>file_name</servlet-name>
        <url-pattern>/file_name</url-pattern>
    </servlet-mapping>

    <!-- mac_who_is -->
    <servlet>
        <servlet-name>mac_who_is</servlet-name>
        <servlet-class>monster.helloworld.mac_who_is.MacWhoIsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mac_who_is</servlet-name>
        <url-pattern>/mac_who_is</url-pattern>
    </servlet-mapping>

    <!-- get_oui_update_time -->
    <servlet>
        <servlet-name>get_oui_update_time</servlet-name>
        <servlet-class>monster.helloworld.mac_who_is.GetOuiUpdateTimeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>get_oui_update_time</servlet-name>
        <url-pattern>/get_oui_update_time</url-pattern>
    </servlet-mapping>
</web-app>